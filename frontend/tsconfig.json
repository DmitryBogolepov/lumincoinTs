{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node",
    "allowJs": true,
    "noImplicitAny": true,
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "*": ["*", "./*"]
    },
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}